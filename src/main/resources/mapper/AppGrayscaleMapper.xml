<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hamsterwhat.wechat.mapper.AppGrayscaleMapper">
    <select id="selectGrayscaleUIDsByAppVersionId" resultType="java.lang.String">
        SELECT grayscale_uid FROM app_grayscale WHERE app_version_id = #{appVersionId}
    </select>
</mapper>